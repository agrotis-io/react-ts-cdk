{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "dist/",
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noEmitOnError": true,
    "noUnusedParameters": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules"
  ],
  "files": [
    "src/components/index.ts"
  ],
}